{
  "lib/scss_lint/*.rb": {
    "alternate": "spec/scss_lint/{}_spec.rb",
    "type": "source",
    "dispatch": "bundle exec rspec spec/scss_lint/{}_spec.rb"
  },
  "spec/scss_lint/*_spec.rb": {
    "alternate": "lib/scss_lint/{}.rb",
    "type": "test",
    "dispatch": "bundle exec rspec {file}"
  }
}
